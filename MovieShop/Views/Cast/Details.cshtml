@model List<CastModel>

<table class="table table-striped">
	<thead class="thead-dark">
		<tr>
			<th scope="col">ID</th>
			<th scope="col">Name</th>
			<th scope="col">Gender</th>
		</tr>
	</thead>
	<tbody>
		@foreach(var cast in Model) {
			<tr>
				<th scope="row">@cast.Id</th>
				<td>@cast.Name</td>
				<td>@cast.Gender</td>
			</tr>
		}
	</tbody>
</table>